Package: rules
Title: Model Wrappers for Rule-Based Models
Version: 0.0.0.9000
Authors@R: c(
    person(
        given = "Max",
        family = "Kuhn",
        email = "max@rstudio.com",
        comment = c(ORCID = "0000-0003-2402-136X"),
        role = c("aut", "cre")
    ),
    person("RStudio", role = "cph")
